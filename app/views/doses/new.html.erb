<div class="container">
  <h1>Doses#new</h1>
  <p>Find me in app/views/doses/new.html.erb</p>
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>Create a new cocktail</h1>
      <%= simple_form_for [@cocktail, @dose] do |f| %>
        <%= f.input :Ingredient %>
        <%= f.input :Description %>
        <%= f.submit class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
